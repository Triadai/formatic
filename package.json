{
  "name": "formatic",
  "version": "0.3.30",
  "description": "Automatic, pluggable form generation",
  "main": "./build/lib/formatic",
  "scripts": {
    "test": "jest",
    "build": "babel lib --out-dir build/lib",
    "docs-build": "gulp docs-build",
    "docs-publish": "gulp docs-publish",
    "live": "node server.js",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zapier/formatic.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.0.5",
    "babel-jest": "^4.0.0",
    "babel-loader": "^5.3.2",
    "babelify": "^5.0.3",
    "bootstrap": "^3.3.5",
    "codemirror": "^5.9.0",
    "css-loader": "^0.23.0",
    "envify": "^1.2.1",
    "es6-promise": "^3.0.2",
    "eslint": "^1.2.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^2.3.0",
    "groc": "https://registry.npmjs.org/groc/-/groc-0.7.0.tgz",
    "gulp": "^3.8.11",
    "gulp-gh-pages": "^0.4.0",
    "gulp-load-plugins": "^0.5.3",
    "gulp-run": "^1.6.6",
    "gulp-shell": "^0.2.8",
    "history": "1.13.x",
    "jest-cli": "^0.4.0",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "react": "^0.14.6",
    "react-addons-test-utils": "^0.14.3",
    "react-addons-update": "^0.14.6",
    "react-bootstrap": "^0.23.0",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-router": "1.0.2",
    "react-tools": "^0.12.2",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.0.2",
    "shelljs": "^0.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "classnames": "^2.1.1",
    "deep-equal": "^1.0.0",
    "object-assign": "^2.0.0",
    "react-addons-css-transition-group": "^0.14.3",
    "react-scroll-lock": "git+https://github.com/Laiff/react-scroll-lock.git#267bf5bcf84d334aecc20908657088007b698dc2"
  },
  "peerDependencies": {
    "react": ">=0.14"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/index.js",
      "<rootDir>/lib",
      "<rootDir>/build"
    ],
    "testPathDirs": [
      "__tests__"
    ]
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim",
      "brfs"
    ]
  },
  "browserify-shim": {
    "react": "global:React",
    "react-addons-css-transition-group": "global:React.addons.CSSTransitionGroup",
    "react-dom": "global:ReactDOM",
    "underscore": "global:_",
    "codemirror": "global:CodeMirror"
  }
}
